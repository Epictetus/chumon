= form_for @order_detail do |f|
  -if @order_detail.errors.any?
    #error_explanation
      %h2= "#{pluralize(@order_detail.errors.count, "error")} prohibited this order_detail from being saved:"
      %ul
        - @order_detail.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :product_id
    = f.number_field :product_id
  .field
    = f.label :order_id
    = f.number_field :order_id
  .field
    = f.label :unit_price
    = f.number_field :unit_price
  .field
    = f.label :quantity
    = f.number_field :quantity
  .actions
    = f.submit 'Save'
